uib-carousel(active="active", interval="getInterval()",
        ng-class="{'hide-controls': hideControls, 'hide-indicators':hideIndicators}")
    uib-slide(ng-repeat="slide in slides track by $index", index="$index")
        img(ng-src="{{slide.image}}")
        .carousel-caption
            h4(ng-show="slide.title") {{slide.title}}
            p(ng-show="slide.text") {{slide.text}}
.display-bottom
img.display-stem(src="/@@__SOURCE_PATH__/stem.png")
